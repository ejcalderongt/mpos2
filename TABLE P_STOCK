	[UNIDADMEDIDA] [nvarchar](10) NOT NULL,
	[ANULADO] [bit] NOT NULL,
	[ENVIADO] [bit] NOT NULL,
	[CODIGOLIQUIDACION] [int] NOT NULL,
	[FECHA_SISTEMA] [datetime] NOT NULL,
	[DOCUMENTO] [nvarchar](50) NOT NULL,
